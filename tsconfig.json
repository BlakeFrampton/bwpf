{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2015", "dom"],
    "module": "none",
    "outFile": "falling-sand/sketch.js",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["p5"]
  },
  "include": [
    "**/*.ts"
  ]
}
